# Define build targets for release and debug builds

# TODO: Use `--target-dir' and `--out-dir'.
#       You need to check other existing usage of `target/{debug,release}'

# TODO: Use environment variable to set the pkgdb binary path
@ENABLE_RELEASE_TRUE@build: copy-srcs
@ENABLE_RELEASE_TRUE@	$(MAKE) $(AM_MAKEFLAGS) -C $(top_builddir)/pkgdb install;
@ENABLE_RELEASE_TRUE@	PKGDB_BIN="$(DESTDIR)$(bindir)/pkgdb$(EXEEXT)"  \
@ENABLE_RELEASE_TRUE@	$(CARGO) build $(CARGOFLAGS);

@ENABLE_RELEASE_FALSE@build: copy-srcs
@ENABLE_RELEASE_FALSE@	$(MAKE) $(AM_MAKEFLAGS) -C $(top_builddir)/pkgdb bin/pkgdb$(EXEEXT)
@ENABLE_RELEASE_FALSE@	$(CARGO) build $(CARGOFLAGS);

